<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>YibTronBackend</Title>
    <PackageId>YibTron.YibTronBackend.DataBases</PackageId>
    <AssemblyName>YibTronBackend</AssemblyName>
    <RootNamespace>YibTronBackend</RootNamespace>
    <Version>1.0.1</Version>
    <Authors>JahzeelAndre</Authors>
    <Product>YibTronBackend</Product>
    <Description>Paquete que facilita la creación de la base de datos y tablas mediante clases heredables con ADO NET.
Clases con herramientas útiles para la comunicación con la base de datos.</Description>
    <PackageProjectUrl>https://github.com/JahzeelAndre/YibTron</PackageProjectUrl>
    <Copyright>Copyright (c) 2023 Jahzeel Andre Yibale Toledo </Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/JahzeelAndre/YibTron</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Backend, SqlServer</PackageTags>
    <PackageReleaseNotes>Utilidades para la creación de bases de datos, tablas y registros en sql server.</PackageReleaseNotes>
    <PackageOutputPath>bin\Release\</PackageOutputPath>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <DebugType>embedded</DebugType>
    <IncludeSymbols>True</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Company>JahzeelAndre</Company>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\..\..\Downloads\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\..\..\..\..\Downloads\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.1.1" />
  </ItemGroup>

</Project>
